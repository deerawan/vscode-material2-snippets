{
  "Autocomplete": {
    "prefix": "mat-auto",
    "body": [
      "<mat-autocomplete #${name}=\"matAutocomplete\">",
      "\t<mat-option></mat-option>",
      "</mat-autocomplete>"
    ],
    "description": "Material 2 Autocomplete"
  },
  "Button Basic": {
    "prefix": "mat-btn",
    "body": [
      "<button mat-button>${text}</button>"
    ],
    "description": "Material 2 Button"
  },
  "Button Basic with Link": {
    "prefix": "mat-btn-link",
    "body": [
      "<a mat-button routerLink=\"${link}\">${text}</a>"
    ],
    "description": "Material 2 Button with Link"
  },
  "Button Raised": {
    "prefix": "mat-raised-btn",
    "body": [
      "<button mat-raised-button>${text}</button>"
    ],
    "description": "Material 2 Raised Button"
  },
  "Button Raised with Link": {
    "prefix": "mat-raised-btn-link",
    "body": [
      "<a mat-raised-button routerLink=\"${link}\">${text}</a>"
    ],
    "description": "Material 2 Raised Button with Link"
  },
  "Button Fab": {
    "prefix": "mat-fab",
    "body": [
      "<button mat-fab><mat-icon>${icon}</mat-icon></button>"
    ],
    "description": "Material 2 FAB Button"
  },
  "Button Fab with Link": {
    "prefix": "mat-fab-link",
    "body": [
      "<a mat-fab routerLink=\"${link}\"><mat-icon>${icon}</mat-icon></a>"
    ],
    "description": "Material 2 FAB Button with Link"
  },
  "Button Mini Fab": {
    "prefix": "mat-mini-fab",
    "body": [
      "<button mat-mini-fab><mat-icon>${icon}</mat-icon></button>"
    ],
    "description": "Material 2 Mini FAB Button"
  },
  "Button Mini Fab with Link": {
    "prefix": "mat-mini-fab-link",
    "body": [
      "<a mat-mini-fab routerLink=\"${link}\"><mat-icon>${icon}</mat-icon></a>"
    ],
    "description": "Material 2 Mini FAB Button with Link"
  },
  "Button Toggle": {
    "prefix": "mat-btn-toggle",
    "body": [
      "<mat-button-toggle>${text}</mat-button-toggle>"
    ],
    "description": "Material 2 Button Toggle"
  },
  "Button Toggle Group": {
    "prefix": "mat-btn-toggle-grp",
    "body": [
      "<mat-button-toggle-group #group=\"${groupName}\">",
      "\t<mat-button-toggle value=\"${value}\">${content}</mat-button-toggle>",
      "</mat-button-toggle-group>"
    ],
    "description": "Material 2 Button Toggle Group"
  },
  "Card": {
    "prefix": "mat-card",
    "body": [
      "<mat-card>${text}</mat-card>"
    ],
    "description": "Material 2 Card"
  },
  "Card Multi Sections": {
    "prefix": "mat-card-section",
    "body": [
      "<mat-card>",
      "\t<mat-card-header>",
      "\t\t<mat-card-title>${title}</mat-card-title>",
      "\t\t<mat-card-subtitle>${subTitle}</mat-card-subtitle>",
      "\t</mat-card-header>",
      "\t<mat-card-content>${content}</mat-card-content>",
      "\t<mat-card-actions>${actions}</mat-card-actions>",
      "</mat-card>"
    ],
    "description": "Material 2 Card with Sections"
  },
  "Checkbox": {
    "prefix": "mat-checkbox",
    "body": [
      "<mat-checkbox>${text}</mat-checkbox>"
    ],
    "description": "Material 2 Checkbox"
  },
  "Chips": {
    "prefix": "mat-chip",
    "body": [
      "<mat-chip-list>",
      "\t<mat-chip>${text}</mat-chip>",
      "</mat-chip-list>"
    ],
    "description": "Material 2 Chips"
  },
  "Date Picker": {
    "prefix": "mat-date-pick",
    "body": [
      "<mat-form-field>",
      "\t<input matInput [matDatepicker]=\"${name}\" placeholder=\"${placeholder}\">",
      "\t<mat-datepicker-toggle matSuffix [for]=\"${name}\"></mat-datepicker-toggle>",
      "\t<mat-datepicker #${name}></mat-datepicker>",
      "</mat-form-field>"
    ]
  },
  "Expansion Panel": {
    "prefix": "mat-exp-panel",
    "body": [
      "<mat-expansion-panel>",
      "\t<mat-expansion-panel-header>",
      "\t\t<mat-panel-title>${title}</mat-panel-title>",
      "\t\t<mat-panel-description>${description}</mat-panel-description>",
      "\t</mat-expansion-panel-header>",
      "\t${primaryContent}",
      "</mat-expansion-panel>"
    ]
  },
  "Grid List": {
    "prefix": "mat-grid",
    "body": [
      "<mat-grid-list cols=\"${cols}\">",
      "\t<mat-grid-tile>${text}</mat-grid-tile>",
      "</mat-grid-list>"
    ],
    "description": "Material 2 Grid List"
  },
  "Icon": {
    "prefix": "mat-icon",
    "body": [
      "<mat-icon>${icon}</mat-icon>"
    ],
    "description": "Material 2 Icon"
  },
  "Icon SVG": {
    "prefix": "mat-icon-svg",
    "body": [
      "<mat-icon svgIcon=\"${icon}\"></mat-icon>"
    ],
    "description": "Material 2 Icon using SVG icon"
  },
  "Input": {
    "prefix": "mat-input",
    "body": [
      "<mat-form-field class=\"${className}\">",
      "\t<input matInput placeholder=\"${placeholder}\">",
      "</mat-form-field>"
    ],
    "description": "Material 2 Input"
  },
  "Input Textarea": {
    "prefix": "mat-textarea",
    "body": [
      "<mat-form-field class=\"${className}\">",
      "\t<textarea matInput placeholder=\"${placeholder}\">${value}</textarea>",
      "</mat-form-field>"
    ],
    "description": "Material 2 Input Textarea"
  },
  "List": {
    "prefix": "mat-list",
    "body": [
      "<mat-list>",
      "\t<mat-list-item>${text}</mat-list-item>",
      "</mat-list>"
    ],
    "Description": "Material 2 List"
  },
  "Menu": {
    "prefix": "mat-menu",
    "body": [
      "<mat-menu #${name}=\"matMenu\">",
      "\t<button mat-menu-item>${text}</button>",
      "</mat-menu>"
    ],
    "Description": "Material 2 Menu"
  },
  "Menu Icon": {
    "prefix": "mat-menu-icon",
    "body": [
      "<mat-menu #${name}=\"matMenu\">",
      "\t<button mat-menu-item>",
      "\t\t<mat-icon>${iconName}</mat-icon>",
      "\t\t<span>${text}</span>",
      "\t</button>",
      "</mat-menu>"
    ],
    "Description": "Material 2 Menu with Icon"
  },
  "Paginator": {
    "prefix": "mat-paginator",
    "body": [
      "<mat-paginator [length]=\"${length}\" [pageSize]=\"${pageSize}\" [pageSizeOptions]=\"${options}\"></mat-paginator>"
    ]
  },
  "Progress Bar": {
    "prefix": "mat-prog-bar",
    "body": "<mat-progress-bar mode=\"${mode}\"></mat-progress-bar>",
    "Description": "Material 2 Progress Bar"
  },
  "Progress Spinner": {
    "prefix": "mat-prog-spin",
    "body": "<mat-progress-spinner mode=\"${mode}\"></mat-progress-spinner>",
    "Description": "Material 2 Progress Spinner"
  },
  "Radio": {
    "prefix": "mat-radio",
    "body": [
      "<mat-radio-group>",
      "\t<mat-radio-button value=\"${value}\">${text}</mat-radio-button>",
      "</mat-radio-group>"
    ],
    "Description": "Material 2 Radio"
  },
  "Select": {
    "prefix": "mat-select",
    "body": [
      "<mat-form-field>",
      "\t<mat-select placeholder=\"${placeholder}\" name=\"${name}\">",
      "\t\t<mat-option></mat-option>",
      "\t</mat-select>",
      "</mat-form-field>"
    ],
    "Description": "Material 2 Select"
  },
  "Sidenav": {
    "prefix": "mat-sidenav",
    "body": [
      "<mat-sidenav-container>",
      "\t<mat-sidenav>${content}</mat-sidenav>",
      "</mat-sidenav-container>"
    ],
    "Description": "Material 2 Side Nav"
  },
  "Slider": {
    "prefix": "mat-slider",
    "body": [
      "<mat-slider></mat-slider>"
    ],
    "Description": "Material 2 Slider"
  },
  "Slide Toogle": {
    "prefix": "mat-slide",
    "body": [
      "<mat-slide-toggle>${text}</mat-slide-toggle>"
    ],
    "Description": "Material 2 Slide Toggle"
  },
  "Stepper": {
    "prefix": "mat-step",
    "body": [
      "<mat-${horizontal}-stepper>",
      "\t<mat-step>${content1}</mat-step>",
      "\t<mat-step>${content2}</mat-step>",
      "</mat-${horizontal}-stepper>"
    ]
  },
  "Tab": {
    "prefix": "mat-tab",
    "body": [
      "<mat-tab-group>",
      "\t<mat-tab label=\"${label}\">${content}</mat-tab>",
      "</mat-tab-group>"
    ],
    "Description": "Material 2 Tab"
  },
  "Toolbar": {
    "prefix": "mat-toolbar",
    "body": [
      "<mat-toolbar>${text}</mat-toolbar>"
    ],
    "Description": "Material 2 Toolbar"
  },
  "Toolbar Multirow": {
    "prefix": "mat-toolbar-row",
    "body": [
      "<mat-toolbar>",
      "\t<mat-toolbar-row>",
      "\t\t<span>${text}</span>",
      "\t\t<span class=\"example-spacer\"></span>",
      "\t\t<mat-icon></mat-icon>",
      "\t</mat-toolbar-row>",
      "</mat-toolbar>"
    ],
    "Description": "Material 2 Toolbar Multi Row"
  },
  "Tooltip": {
    "prefix": "mat-tooltip",
    "body": [
      "<${dom} matTooltip=\"Tooltip!\"></${dom}>"
    ]
  }
}
