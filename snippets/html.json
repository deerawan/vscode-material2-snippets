{
  "Button Basic": {
    "prefix": "md2-btn",
    "body": [
      "<button md-button>${text}</button>"
    ],
    "description": "Material 2 Button"
  },
  "Button Basic with Link": {
    "prefix": "md2-btn-link",
    "body": [
      "<a md-button routerLink=\"${link}\">${text}</a>"
    ],
    "description": "Material 2 Button with Link"
  },
  "Button Icon": {
    "prefix": "md2-icon-btn",
    "body": [
      "<button md-icon-button><md-icon>${icon}</md-icon></button>"
    ],
    "description": "Material 2 Icon Button"
  },
  "Button Raised": {
    "prefix": "md2-raised-btn",
    "body": [
      "<button md-raised-button>${content}</button>"
    ],
    "description": "Material 2 Raised Button"
  },
  "Button Raised with Link": {
    "prefix": "md2-raised-btn-link",
    "body": [
      "<a md-raised-button routerLink=\"${link}\">${text}</a>"
    ],
    "description": "Material 2 Raised Button with Link"
  },
  "Button Fab": {
    "prefix": "md2-fab",
    "body": [
      "<button md-fab><md-icon>${icon}</md-icon></button>"
    ],
    "description": "Material 2 FAB Button"
  },
  "Button Fab with Link": {
    "prefix": "md2-fab-link",
    "body": [
      "<a md-fab routerLink=\"${link}\"><md-icon>${icon}</md-icon></a>"
    ],
    "description": "Material 2 FAB Button with Link"
  },
  "Button Mini Fab": {
    "prefix": "md2-mini-fab",
    "body": [
      "<button md-mini-fab><md-icon>${icon}</md-icon></button>"
    ],
    "description": "Material 2 Mini FAB Button"
  },
  "Button Mini Fab with Link": {
    "prefix": "md2-mini-fab-link",
    "body": [
      "<a md-mini-fab routerLink=\"${link}\"><md-icon>${icon}</md-icon></a>"
    ],
    "description": "Material 2 Mini FAB Button with Link"
  },
  "Button Toggle": {
    "prefix": "md2-btn-toggle",
    "body": [
      "<md-button-toggle>${text}</md-button-toggle>"
    ],
    "description": "Material 2 Button Toggle"
  },
  "Button Toggle Group": {
    "prefix": "md2-btn-toggle-group",
    "body": [
      "<md-button-toggle-group #group=\"${groupName}\">",
      "\t<md-button-toggle value=\"${value}\">${content}</md-button-toggle>",
      "</md-button-toggle-group>"
    ],
    "description": "Material 2 Button Toggle Group"
  },
  "Card": {
    "prefix": "md2-card",
    "body": [
      "<md-card>",
      "\t<md-card-title>${title}</md-card-title>",
      "\t<md-card-content>${content}</md-card-content>",
      "</md-card>"
    ],
    "description": "Material 2 Card"
  },
  "Checkbox": {
    "prefix": "md2-checkbox",
    "body": [
      "<md-checkbox>${content}</md-checkbox>"
    ],
    "description": "Material 2 Checkbox"
  },
  "Chips": {
    "prefix": "md2-chip",
    "body": [
      "<md-chip-list>",
      "\t<md-chip>${text}</md-chip>",
      "</md-chip-list>"
    ],
    "description": "Material 2 Chips"
  },
  "Grid List": {
    "prefix": "md2-grid-list",
    "body": [
      "<md-grid-list>",
      "\t<md-grid-tile>",
      "\t\t<md-grid-tile-header>${header}</md-grid-tile-header>",
      "\t\t<md-grid-tile-footer>${footer}</md-grid-tile-footer>",
      "\t</md-grid-tile>",
      "</md-grid-list>"
    ],
    "description": "Material 2 Grid List"
  },
  "Icon": {
    "prefix": "md2-icon",
    "body": [
      "<md-icon>${icon}</md-icon>"
    ],
    "description": "Material 2 Icon"
  },
  "Icon SVG": {
    "prefix": "md2-icon-svg",
    "body": [
      "<md-icon svgIcon=\"${icon}\"></md-icon>"
    ],
    "description": "Material 2 Icon using SVG icon"
  },
  "Icon SVG Src": {
    "prefix": "md2-icon-src",
    "body": [
      "<md-icon svgSrc=\"${iconSrc}.svg\"></md-icon>"
    ],
    "description": "Material 2 Icon using SVG src"
  },
  "Icon Font": {
    "prefix": "md2-icon-font",
    "body": [
      "<md-icon fontSet=\"${fontSet}\" fontIcon=\"${fontIcon}\"></md-icon>"
    ],
    "description": "Material 2 Icon using Font set"
  },
  "Input": {
    "prefix": "md2-input",
    "body": [
      "<md-input placeholder=\"${placeholder}\" value=\"${value}\"></md-input>"
    ],
    "description": "Material 2 Input"
  },
  "Input with hint": {
    "prefix": "md2-input-hint",
    "body": [
      "<md-input placeholder=\"${placeholder}\" value=\"${value}\">",
      "\t<md-hint align=\"${align}\"></md-hint>",
      "</md-input>"
    ],
    "description": "Material 2 Input with hint"
  },
  "List": {
    "prefix": "md2-list",
    "body": [
      "<md-list>",
      "\t<md-list-item *ngFor=\"${ngFor}\">",
      "\t\t<p md-line></p>",
      "\t</md-list-item>",
      "</md-list>"
    ],
    "Description": "Material 2 List"
  },
  "List Dense": {
    "prefix": "md2-dense-list",
    "body": [
      "<md-list dense>",
      "\t<md-list-item *ngFor=\"${ngFor}\">",
      "\t\t<p md-line></p>",
      "\t</md-list-item>",
      "</md-list>"
    ],
    "Description": "Material 2 Dense List"
  },
  "List Nav": {
    "prefix": "md2-nav-list",
    "body": [
      "<md-nav-list>",
      "\t<md-list-item *ngFor=\"${ngFor}\">",
      "\t\t<p md-line></p>",
      "\t</md-list-item>",
      "</md-nav-list>"
    ],
    "Description": "Material 2 Nav List"
  },
  "Progress Bar": {
    "prefix": "md2-progress-bar",
    "body": "<md-progress-bar mode=\"${mode}\" [value]=\"${value}\"></md-progress-bar>",
    "Description": "Material 2 Progress Bar"
  },
  "Progress Circle": {
    "prefix": "md2-progress-circle",
    "body": "<md-progress-circle mode=\"${mode}\" [value]=\"${value}\"></md-progress-circle>",
    "Description": "Material 2 Progress Circle"
  },
  "Radio": {
    "prefix": "md2-radio",
    "body": "<md-radio-button name=\"${name}\"></md-radio-button>",
    "Description": "Material 2 Radio Button"
  },
  "Radio Group": {
    "prefix": "md2-radio-group",
    "body": [
      "<md-radio-group name=\"${name}\">",
      "\t<md-radio-button></md-radio-button>",
      "</md-radio-group>"
    ],
    "Description": "Material 2 Radio Group"
  },
  "Sidenav": {
    "prefix": "md2-sidenav",
    "body": [
      "<md-sidenav-layout>",
      "\t<md-sidenav>${content}</md-sidenav>",
      "</md-sidenav-layout>"
    ],
    "Description": "Material 2 Side Nav"
  },
  "Tab": {
    "prefix": "md2-tab",
    "body": [
      "<md-tab-group>",
      "\t<md-tab>${content}</md-tab>",
      "</md-tab-group>"
    ],
    "Description": "Material 2 Tab"
  }
}