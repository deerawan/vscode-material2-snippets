{
  "Autocomplete": {
    "prefix": "md2-auto",
    "body": [
      "<md-autocomplete #${name}=\"mdAutocomplete\">",
      "\t<md-option></md-option>",
      "</md-autocomplete>"
    ],
    "description": "Material 2 Autocomplete"
  },
  "Button Basic": {
    "prefix": "md2-btn",
    "body": [
      "<button md-button>${text}</button>"
    ],
    "description": "Material 2 Button"
  },
  "Button Basic with Link": {
    "prefix": "md2-btn-link",
    "body": [
      "<a md-button routerLink=\"${link}\">${text}</a>"
    ],
    "description": "Material 2 Button with Link"
  },
  "Button Raised": {
    "prefix": "md2-raised-btn",
    "body": [
      "<button md-raised-button>${text}</button>"
    ],
    "description": "Material 2 Raised Button"
  },
  "Button Raised with Link": {
    "prefix": "md2-raised-btn-link",
    "body": [
      "<a md-raised-button routerLink=\"${link}\">${text}</a>"
    ],
    "description": "Material 2 Raised Button with Link"
  },
  "Button Fab": {
    "prefix": "md2-fab",
    "body": [
      "<button md-fab><md-icon>${icon}</md-icon></button>"
    ],
    "description": "Material 2 FAB Button"
  },
  "Button Fab with Link": {
    "prefix": "md2-fab-link",
    "body": [
      "<a md-fab routerLink=\"${link}\"><md-icon>${icon}</md-icon></a>"
    ],
    "description": "Material 2 FAB Button with Link"
  },
  "Button Mini Fab": {
    "prefix": "md2-mini-fab",
    "body": [
      "<button md-mini-fab><md-icon>${icon}</md-icon></button>"
    ],
    "description": "Material 2 Mini FAB Button"
  },
  "Button Mini Fab with Link": {
    "prefix": "md2-mini-fab-link",
    "body": [
      "<a md-mini-fab routerLink=\"${link}\"><md-icon>${icon}</md-icon></a>"
    ],
    "description": "Material 2 Mini FAB Button with Link"
  },
  "Button Toggle": {
    "prefix": "md2-btn-toggle",
    "body": [
      "<md-button-toggle>${text}</md-button-toggle>"
    ],
    "description": "Material 2 Button Toggle"
  },
  "Button Toggle Group": {
    "prefix": "md2-btn-toggle-grp",
    "body": [
      "<md-button-toggle-group #group=\"${groupName}\">",
      "\t<md-button-toggle value=\"${value}\">${content}</md-button-toggle>",
      "</md-button-toggle-group>"
    ],
    "description": "Material 2 Button Toggle Group"
  },
  "Card": {
    "prefix": "md2-card",
    "body": [
      "<md-card>${text}</md-card>"
    ],
    "description": "Material 2 Card"
  },
  "Card Multi Sections": {
    "prefix": "md2-card-section",
    "body": [
      "<md-card>",
      "\t<md-card-header>",
      "\t\t<md-card-title>${title}</md-card-title>",
      "\t\t<md-card-subtitle>${subTitle}</md-card-subtitle>",
      "\t</md-card-header>",
      "\t<md-card-content>${content}</md-card-content>",
      "\t<md-card-actions>${actions}</md-card-actions>",
      "</md-card>"
    ],
    "description": "Material 2 Card with Sections"
  },
  "Checkbox": {
    "prefix": "md2-checkbox",
    "body": [
      "<md-checkbox>${text}</md-checkbox>"
    ],
    "description": "Material 2 Checkbox"
  },
  "Chips": {
    "prefix": "md2-chip",
    "body": [
      "<md-chip-list>",
      "\t<md-chip>${text}</md-chip>",
      "</md-chip-list>"
    ],
    "description": "Material 2 Chips"
  },
  "Grid List": {
    "prefix": "md2-grid",
    "body": [
      "<md-grid-list cols=\"${cols}\">",
      "\t<md-grid-tile>${text}</md-grid-tile>",
      "</md-grid-list>"
    ],
    "description": "Material 2 Grid List"
  },
  "Icon": {
    "prefix": "md2-icon",
    "body": [
      "<md-icon>${icon}</md-icon>"
    ],
    "description": "Material 2 Icon"
  },
  "Icon SVG": {
    "prefix": "md2-icon-svg",
    "body": [
      "<md-icon svgIcon=\"${icon}\"></md-icon>"
    ],
    "description": "Material 2 Icon using SVG icon"
  },
  "Input": {
    "prefix": "md2-input",
    "body": [
      "<md-input-container>",
      "\t<input mdInput placeholder=\"${placeholder}\">",
      "</md-input-container>"
    ],
    "description": "Material 2 Input"
  },
  "Input Textarea": {
    "prefix": "md2-textarea",
    "body": [
      "<md-input-container>",
      "\t<textarea mdInput placeholder=\"${placeholder}\">${value}</textarea>",
      "</md-input-container>"
    ],
    "description": "Material 2 Input Textarea"
  },
  "List": {
    "prefix": "md2-list",
    "body": [
      "<md-list>",
      "\t<md-list-item>${text}</md-list-item>",
      "</md-list>"
    ],
    "Description": "Material 2 List"
  },
  "Menu": {
    "prefix": "md2-menu",
    "body": [
      "<md-menu #${name}=\"mdMenu\">",
      "\t<button md-menu-item>${text}</button>",
      "</md-menu>"
    ],
    "Description": "Material 2 Menu"
  },
  "Menu Icon": {
    "prefix": "md2-menu-icon",
    "body": [
      "<md-menu #${name}=\"mdMenu\">",
      "\t<button md-menu-item>",
      "\t\t<md-icon>${iconName}</md-icon>",
      "\t\t<span>${text}</span>",
      "\t</button>",
      "</md-menu>"
    ],
    "Description": "Material 2 Menu with Icon"
  },
  "Progress Bar": {
    "prefix": "md2-prog-bar",
    "body": "<md-progress-bar mode=\"${mode}\"></md-progress-bar>",
    "Description": "Material 2 Progress Bar"
  },
  "Progress Spinner": {
    "prefix": "md2-spinner",
    "body": "<md-spinner></md-spinner>",
    "Description": "Material 2 Spinner"
  },
  "Progress Circle": {
    "prefix": "md2-prog-circle",
    "body": "<md-progress-circle mode=\"${mode}\" [value]=\"${value}\" [color]=\"${color}\"></md-progress-circle>",
    "Description": "Material 2 Progress Circle"
  },
  "Radio": {
    "prefix": "md2-radio",
    "body": [
      "<md-radio-group>",
      "\t<md-radio-button></md-radio-button>",
      "</md-radio-group>"
    ],
    "Description": "Material 2 Radio"
  },
  "Select": {
    "prefix": "md2-select",
    "body": [
      "<md-select placeholder=\"${placeholder}\" name=\"${name}\">",
      "\t<md-option></md-option>",
      "</md-select>"
    ],
    "Description": "Material 2 Select"
  },
  "Sidenav": {
    "prefix": "md2-sidenav",
    "body": [
      "<md-sidenav-container>",
      "\t<md-sidenav>${content}</md-sidenav>",
      "</md-sidenav-container>"
    ],
    "Description": "Material 2 Side Nav"
  },
  "Slider": {
    "prefix": "md2-slider",
    "body": [
      "<md-slider></md-slider>"
    ],
    "Description": "Material 2 Slider"
  },
  "Slide Toogle": {
    "prefix": "md2-slide",
    "body": [
      "<md-slide-toggle>${text}</md-slide-toggle>"
    ],
    "Description": "Material 2 Slide Toggle"
  },
  "Tab": {
    "prefix": "md2-tab",
    "body": [
      "<md-tab-group>",
      "\t<md-tab label=\"${label}\">${content}</md-tab>",
      "</md-tab-group>"
    ],
    "Description": "Material 2 Tab"
  },
  "Toolbar": {
    "prefix": "md2-toolbar",
    "body": [
      "<md-toolbar>${text}</md-toolbar>"
    ],
    "Description": "Material 2 Toolbar"
  },
  "Toolbar Multirow": {
    "prefix": "md2-toolbar-row",
    "body": [
      "<md-toolbar>",
      "\t<md-toolbar-row>",
      "\t\t<span>${text}</span>",
      "\t\t<span class=\"example-spacer\"></span>",
      "\t\t<md-icon></md-icon>",
      "\t</md-toolbar-row>",
      "</md-toolbar>"
    ],
    "Description": "Material 2 Toolbar Multi Row"
  },
  "Tooltip": {
    "prefix": "md2-tooltip",
    "body": [
      "<${dom} mdTooltip=\"Tooltip!\"></${dom}>"
    ]
  }
}
