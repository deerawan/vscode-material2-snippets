{
  "Autocomplete": {
    "prefix": "mat-autocomplete",
    "body": [
      "<mat-autocomplete #auto=\"matAutocomplete\">",
      "\t<mat-option *ngFor=\"let ${2:element} of ${1:array}\" [value]=\"${2:element}\">{{${2:element}}}</mat-option>",
      "</mat-autocomplete>"
    ],
    "description": "Material Autocomplete"
  },
  "Button Basic": {
    "prefix": "md2-btn",
    "body": [
      "<button md-button>${text}</button>"
    ],
    "description": "Material 2 Button"
  },
  "Button Basic with Link": {
    "prefix": "md2-btn-link",
    "body": [
      "<a md-button routerLink=\"${link}\">${text}</a>"
    ],
    "description": "Material 2 Button with Link"
  },
  "Button Raised": {
    "prefix": "md2-raised-btn",
    "body": [
      "<button md-raised-button>${text}</button>"
    ],
    "description": "Material 2 Raised Button"
  },
  "Button Raised with Link": {
    "prefix": "md2-raised-btn-link",
    "body": [
      "<a md-raised-button routerLink=\"${link}\">${text}</a>"
    ],
    "description": "Material 2 Raised Button with Link"
  },
  "Button Fab": {
    "prefix": "md2-fab",
    "body": [
      "<button md-fab><md-icon>${icon}</md-icon></button>"
    ],
    "description": "Material 2 FAB Button"
  },
  "Button Fab with Link": {
    "prefix": "md2-fab-link",
    "body": [
      "<a md-fab routerLink=\"${link}\"><md-icon>${icon}</md-icon></a>"
    ],
    "description": "Material 2 FAB Button with Link"
  },
  "Button Mini Fab": {
    "prefix": "md2-mini-fab",
    "body": [
      "<button md-mini-fab><md-icon>${icon}</md-icon></button>"
    ],
    "description": "Material 2 Mini FAB Button"
  },
  "Button Mini Fab with Link": {
    "prefix": "md2-mini-fab-link",
    "body": [
      "<a md-mini-fab routerLink=\"${link}\"><md-icon>${icon}</md-icon></a>"
    ],
    "description": "Material 2 Mini FAB Button with Link"
  },
  "Button Toggle": {
    "prefix": "md2-btn-toggle",
    "body": [
      "<md-button-toggle>${text}</md-button-toggle>"
    ],
    "description": "Material 2 Button Toggle"
  },
  "Button Toggle Group": {
    "prefix": "md2-btn-toggle-grp",
    "body": [
      "<md-button-toggle-group #group=\"${groupName}\">",
      "\t<md-button-toggle value=\"${value}\">${content}</md-button-toggle>",
      "</md-button-toggle-group>"
    ],
    "description": "Material 2 Button Toggle Group"
  },
  "Card": {
    "prefix": "md2-card",
    "body": [
      "<md-card>${text}</md-card>"
    ],
    "description": "Material 2 Card"
  },
  "Card Multi Sections": {
    "prefix": "md2-card-section",
    "body": [
      "<md-card>",
      "\t<md-card-header>",
      "\t\t<md-card-title>${title}</md-card-title>",
      "\t\t<md-card-subtitle>${subTitle}</md-card-subtitle>",
      "\t</md-card-header>",
      "\t<md-card-content>${content}</md-card-content>",
      "\t<md-card-actions>${actions}</md-card-actions>",
      "</md-card>"
    ],
    "description": "Material 2 Card with Sections"
  },
  "Checkbox": {
    "prefix": "mat-checkbox",
    "body": [
      "<mat-checkbox>${text}</mat-checkbox>"
    ],
    "description": "Material Checkbox"
  },
  "Chips": {
    "prefix": "md2-chip",
    "body": [
      "<md-chip-list>",
      "\t<md-chip>${text}</md-chip>",
      "</md-chip-list>"
    ],
    "description": "Material 2 Chips"
  },
  "Datepicker": {
    "prefix": "mat-datepicker",
    "body": [
      "<mat-form-field>",
      "\t<mat-label>${1:label}</mat-label>",
      "\t<input matInput [matDatepicker]=\"${2:name}\">",
      "\t<mat-datepicker-toggle matSuffix [for]=\"${2:name}\"></mat-datepicker-toggle>", 
      "\t<mat-datepicker #${2:name}></mat-datepicker>",     
      "</mat-form-field>"
    ],
    "description": "Material Datepicker"
  },
  "Grid List": {
    "prefix": "md2-grid",
    "body": [
      "<md-grid-list cols=\"${cols}\">",
      "\t<md-grid-tile>${text}</md-grid-tile>",
      "</md-grid-list>"
    ],
    "description": "Material 2 Grid List"
  },
  "Icon": {
    "prefix": "md2-icon",
    "body": [
      "<md-icon>${icon}</md-icon>"
    ],
    "description": "Material 2 Icon"
  },
  "Icon SVG": {
    "prefix": "md2-icon-svg",
    "body": [
      "<md-icon svgIcon=\"${icon}\"></md-icon>"
    ],
    "description": "Material 2 Icon using SVG icon"
  },
  "Input": {
    "prefix": "mat-input",
    "body": [
      "<input matInput type=\"${1:type}\" placeholder=\"${2:placeholder}\">"
    ],
    "description": "Material Input"
  },
  "Input Form Field": {
    "prefix": "mat-input-field",
    "body": [
      "<mat-form-field>",
      "\t<mat-label>${1:label}</mat-label>",
      "\t<input matInput type=\"${2:type}\" placeholder=\"${3:placeholder}\">",
      "</mat-form-field>"
    ],
    "description": "Material Input with form field and label"
  },
  "Textarea": {
    "prefix": "mat-textarea",
    "body": [
      "<textarea matInput placeholder=\"${placeholder}\"></textarea>"      
    ],
    "description": "Material Textarea"
  },
  "Textarea Form Field": {
    "prefix": "mat-textarea-field",
    "body": [
      "<mat-form-field>",
      "\t<mat-label>${1:label}</mat-label>",
      "\t<textarea matInput placeholder=\"${2:placeholder}\"></textarea>",
      "</mat-form-field>"
    ],
    "description": "Material Textarea with form field and label"
  },
  "List": {
    "prefix": "md2-list",
    "body": [
      "<md-list>",
      "\t<md-list-item>${text}</md-list-item>",
      "</md-list>"
    ],
    "Description": "Material 2 List"
  },
  "Menu": {
    "prefix": "mat-menu",
    "body": [
      "<button mat-button [matMenuTriggerFor]=\"${1:menu}\">${2:buttonLabel}</button>",
      "<mat-menu #${1:menu}=\"matMenu\">",
      "\t<button mat-menu-item>${3:item}</button>",
      "</mat-menu>"
    ],
    "Description": "Material Menu"
  },  
  "Progress Bar": {
    "prefix": "md2-prog-bar",
    "body": "<md-progress-bar mode=\"${mode}\"></md-progress-bar>",
    "Description": "Material 2 Progress Bar"
  },
  "Progress Spinner": {
    "prefix": "md2-spinner",
    "body": "<md-spinner></md-spinner>",
    "Description": "Material 2 Spinner"
  },
  "Progress Circle": {
    "prefix": "md2-prog-circle",
    "body": "<md-progress-circle mode=\"${mode}\" [value]=\"${value}\" [color]=\"${color}\"></md-progress-circle>",
    "Description": "Material 2 Progress Circle"
  },
  "Radio": {
    "prefix": "mat-radio",
    "body": [
      "<mat-radio-group aria-label=\"${1:aria label}\">",
      "\t<mat-radio-button value=\"${2:first value}\">${3:first label}</mat-radio-button>",
      "</mat-radio-group>"
    ],
    "Description": "Material Radio Button"
  },
  "Select": {
    "prefix": "mat-select",
    "body": [
      "<mat-select>",
      "\t<mat-option *ngFor=\"let ${2:element} of ${1:list}\" [value]=\"${3:value}\">",
      "\t\t{{${4:view value}}}",
      "\t</mat-option>",
      "</mat-select>"
    ],
    "Description": "Material Select"
  },
  "Select With Form Field": {
    "prefix": "mat-select-field",
    "body": [
      "<mat-form-field>",
      "\t<mat-label>${1:label}</mat-label>",
      "\t<mat-select>",
      "\t\t<mat-option *ngFor=\"let ${3:element} of ${2:list}\" [value]=\"${4:value}\">",
      "\t\t\t{{${5:view value}}}",
      "\t\t</mat-option>",
      "\t</mat-select>",
      "</mat-form-field>"
    ],
    "Description": "Material Select with form field"
  },
  "Sidenav": {
    "prefix": "md2-sidenav",
    "body": [
      "<md-sidenav-container>",
      "\t<md-sidenav>${content}</md-sidenav>",
      "</md-sidenav-container>"
    ],
    "Description": "Material 2 Side Nav"
  },
  "Slider": {
    "prefix": "md2-slider",
    "body": [
      "<md-slider></md-slider>"
    ],
    "Description": "Material 2 Slider"
  },
  "Slide Toogle": {
    "prefix": "md2-slide",
    "body": [
      "<md-slide-toggle>${text}</md-slide-toggle>"
    ],
    "Description": "Material 2 Slide Toggle"
  },
  "Tab": {
    "prefix": "md2-tab",
    "body": [
      "<md-tab-group>",
      "\t<md-tab label=\"${label}\">${content}</md-tab>",
      "</md-tab-group>"
    ],
    "Description": "Material 2 Tab"
  },
  "Toolbar": {
    "prefix": "md2-toolbar",
    "body": [
      "<md-toolbar>${text}</md-toolbar>"
    ],
    "Description": "Material 2 Toolbar"
  },
  "Toolbar Multirow": {
    "prefix": "md2-toolbar-row",
    "body": [
      "<md-toolbar>",
      "\t<md-toolbar-row>",
      "\t\t<span>${text}</span>",
      "\t\t<span class=\"example-spacer\"></span>",
      "\t\t<md-icon></md-icon>",
      "\t</md-toolbar-row>",
      "</md-toolbar>"
    ],
    "Description": "Material 2 Toolbar Multi Row"
  },
  "Tooltip": {
    "prefix": "md2-tooltip",
    "body": [
      "<${dom} mdTooltip=\"Tooltip!\"></${dom}>"
    ]
  }
}
